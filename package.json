    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
